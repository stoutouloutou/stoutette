upstart :

switch to manual mode for:
xbmc : sudo sh -c "echo 'manual' > /etc/init/xbmc.override"
wd (watch dog) : sudo sh -c "echo 'manual' > /etc/init/wd.override"

/etc/init/pitravel.conf

# pi travel scripts
#
description     "stoutette"
author          "None"

exec  /home/pi/pitravel/stoutette/MAIN

/etc:init/stoutette.conf
# pi travel scripts
#
description     "Main"
author          "None"

# When to start the service
start on runlevel [2345]

# When to stop the service
stop on runlevel [016]

exec  /home/pi/pitravel/stoutette/stoutette
console output

